# Maintainer: EndeavourOS-Team <info@endeavouros.com>

pkgname=yad-eos
depends=(yad)

pkgdesc="A fork of zenity - display graphical dialogs from shell scripts or command line"
url="https://github.com/v1cont/yad"
license=('GPL3')
arch=('x86_64')

_pkgversiondata=$(LANG=C pacman -Si yad | grep -w ^Version)
pkgver=$(echo "$_pkgversiondata" | sed 's|.* \([^-]*\)-\([0-9][0-9.]*\)|\1|')
pkgrel=$(echo "$_pkgversiondata" | sed 's|.* \([^-]*\)-\([0-9][0-9.]*\)|\2|').1
